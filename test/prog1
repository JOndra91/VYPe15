int random(void);
void doSomething(void);

void multiparamFunction(int a, char b, string c) {
  print(a, ' ',  b, ' ', c, '\n');
}

int main(void) {
    int a, vysl;
    if (a>0) {
    }
    else {
        vysl = 1;
        while(a > 0) {
            vysl = vysl * a;
            a=a-1;
        }
    }
    print(vysl, '\n');

    int r;
    r = random();
    doSomething();
    print(r, '\n');

    multiparamFunction(42, 'X', "Universe");
}

